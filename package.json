{
  "name": "censortracker",
  "version": "4.6.0.0",
  "description": "|",
  "author": "Roskomsvoboda",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build:chrome": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development BROWSER=chrome webpack --progress --config webpack.config.js",
    "build:firefox": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development BROWSER=firefox webpack --progress --config webpack.config.js",
    "build:chrome:prod": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production BROWSER=chrome BUILDUP=0 webpack --progress --config webpack.config.js",
    "build:firefox:prod": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production BROWSER=firefox BUILDUP=0  webpack --progress --config webpack.config.js",
    "release:firefox": "web-ext build -o --source-dir=./dist/firefox/prod --artifacts-dir=./releases/firefox",
    "release:chrome": "web-ext build -o --source-dir=./dist/chrome/prod --artifacts-dir=./releases/chrome",
    "release": "web-ext build -o --source-dir=./dist/firefox/prod --artifacts-dir=./releases/firefox && web-ext build -o --source-dir=./dist/chrome/prod --artifacts-dir=./releases/chrome",
    "lint": "eslint --ext=.js ./src/",
    "stylelint": "stylelint ./src/",
    "lint:fix": "eslint --fix --ext=.js ./src/ tests/",
    "test": "jest && rm -f key.pem tests/e2e/selenium/extension/dist.crx",
    "test:unit": "jest tests/unit",
    "test:e2e": "jest tests/e2e/ && rm -f key.pem tests/e2e/selenium/extension/dist.crx",
    "clean": " rm -rf dist releases"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "codemirror": "^5.60.0",
    "ip-range-check": "^0.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@atomix/eslint-config": "^7.0.0-next.1",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "fs": "0.0.1-security",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "merge-jsons-webpack-plugin": "^2.0.0-alpha",
    "prettier": "^2.0.5",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.3.0",
    "terser-webpack-plugin": "^3.0.7",
    "web-ext": "^6.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "zip-webpack-plugin": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roskomsvoboda/censortracker.git"
  },
  "bugs": {
    "url": "https://github.com/roskomsvoboda/censortracker/issues"
  },
  "homepage": "https://github.com/roskomsvoboda/censortracker#readme",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:fix"
    }
  }
}
