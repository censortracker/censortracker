{
  "name": "Censor Tracker",
  "version": "2.0.5.393",
  "description": "Censor Tracker",
  "permissions": [
    "tabs",
    "proxy",
    "storage",
    "activeTab",
    "webRequest",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "declarativeContent",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_name": "Censor Tracker",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons/16x16/default.png",
      "48": "images/icons/48x48/default.png",
      "128": "images/icons/128x128/default.png"
    }
  },
  "options_page": "options.html",
  "manifest_version": 2
}