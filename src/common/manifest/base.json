{
  "name": "Censor Tracker",
  "description": "Censor Tracker",
  "author": "Roskomsvoboda",
  "homepage_url": "https://censortracker.org/",
  "permissions": [
    "tabs",
    "proxy",
    "storage",
    "activeTab",
    "management",
    "webRequest",
    "notifications",
    "unlimitedStorage",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.min.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/icons/16x16/default.png",
    "48": "images/icons/48x48/default.png",
    "128": "images/icons/128x128/default.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "manifest_version": 2,
  "default_locale": "en"
}
